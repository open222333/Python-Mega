

[LOG]
; ******log設定******
; 關閉log功能 輸入選項 (true, True, 1) 預設 不關閉
; LOG_DISABLE=1

; logs路徑 預設 logs
; LOG_PATH=

; 關閉紀錄log檔案 輸入選項 (true, True, 1)  預設 不關閉
; LOG_FILE_DISABLE=1

; 設定紀錄log等級 DEBUG,INFO,WARNING,ERROR,CRITICAL 預設WARNING
; LOG_LEVEL=

[SETTING]
; 下載放置資料夾位置 預設 output
; OUTPUT_PATH=

; 下載m3u8 json檔路徑 預設 conf/m3u8_source.json
; M3U8_JSON_PATH=

; MEGA_ACCOUNT=
; MEGA_PASSWORD=
; MEGA_LISTEN_DIR=